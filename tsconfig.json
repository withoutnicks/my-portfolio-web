{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@pages/*": ["./src/pages/*"],
      "@styles/*": ["./src/styles/*"],
      "@lib/*": ["./src/lib/*"],
      "@ui/*": ["./src/components/ui/*"],
      "@icons/*": ["./src/components/icons/*"],
      "@astro/*": ["./src/components/astro/*"],
      "@views/*": ["./src/components/views/*"]
    }
  }
}
